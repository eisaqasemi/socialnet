{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins",
      "../node_modules/loopback-ds-timestamp-mixin"
    ]
  },
  "User": {
    "dataSource": "mysql",
    "public": true
  },
  "AccessToken": {
    "dataSource": "mysql",
    "public": false
  },
  "ACL": {
    "dataSource": "mysql",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mysql",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mysql",
    "public": false
  },
  "photo": {
    "dataSource": "mysql",
    "public": true
  },
  "tag": {
    "dataSource": "mysql",
    "public": true
  },
  "album": {
    "dataSource": "mysql",
    "public": true
  },
  "upload": {
    "dataSource": "storage",
    "public": true
  },
  "photoTag": {
    "dataSource": "mysql",
    "public": true
  }
}
