{
    "name": "SocialNet",
    "manifest_version": 2,
    "version": "1.0",
    "browser_action": {
      "default_icon": "assets/img/socialnet.png",
      "default_title": "SocialNet",      // optional; shown in tooltip
      "default_popup": "index.html"        // optional
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "permissions": [
      "http://*/"
    ]
  }
